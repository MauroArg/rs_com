<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="rs_comPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.rs_com.entity.Municipio</class>
    <class>com.rs_com.entity.CentroVotacion</class>
    <class>com.rs_com.entity.Votos</class>
    <class>com.rs_com.entity.Candidato</class>
    <class>com.rs_com.entity.Junta</class>
    <class>com.rs_com.entity.Partido</class>
    <class>com.rs_com.entity.Departamento</class>
    <class>com.rs_com.entity.Usuario</class>
    <class>com.rs_com.entity.Rol</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/bdrs_com?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
    </properties>
  </persistence-unit>
</persistence>
